<!DOCTYPE html>
<html ng-app="myApp">
<head>
    <title>ng-repeat Example</title>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.8.2/angular.min.js"></script>
</head>
<body ng-controller="myCtrl">
    <h2>Student Details</h2>
    <table border="1" cellpadding="8">
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Course</th>
        </tr>
        <!-- ng-repeat to fetch details -->
        <tr ng-repeat="student in students">
            <td>{{ student.name }}</td>
            <td>{{ student.age }}</td>
            <td>{{ student.course }}</td>
        </tr>
    </table>
    <script>
        var app = angular.module("myApp", []);
        app.controller("myCtrl", function ($scope) {
            $scope.students = [
                { name: "Rahul", age: 22, course: "BCA" },
                { name: "Sneha", age: 21, course: "BBA" },
                { name: "Amit", age: 23, course: "B.Tech" },
                { name: "Priya", age: 20, course: "BA" }
            ];
        });
    </script>
</body>
</html>

Q2
const express = require('express');
const app = express();

// built-in middlewares
app.use(express.json());              
app.use(express.urlencoded({extended:true})); 

// simple validation middleware
function validateUser(req, res, next) {
  const { name, age } = req.body;
  if (!name || !age) return res.status(400).send("name and age required");
  next();  // valid â†’ continue
}
// route using validation middleware
app.post('/user', validateUser, (req, res) => {
  res.send("User data OK: " + JSON.stringify(req.body));
});
app.listen(3000, () => console.log("Server running on 3000"));


npm init -y
npm install express
node app.js

